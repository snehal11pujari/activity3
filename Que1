package com.question1;
public class AdditionOfReverseNumber extends SumOfDigits {

	public static void main(String[] args) {
		ReverseNumber r=new ReverseNumber();
		SumOfDigits s=new SumOfDigits();
		
		r.reverseNo();
		s.SumOfDigits();

	}

}
